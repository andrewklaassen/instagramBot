{
  "compilerOptions": {
    "lib": [
      "es6"
    ],
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "noImplicitAny": false,
    "outDir": "dist/",
    "types": [
      "node",
      "webdriverio",
      "chai",
      "mocha"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false
}